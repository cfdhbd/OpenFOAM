FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    T
    {
        solver          PCG;  //Preconditioned conjugate gradient method
        preconditioner  DIC;  //Diagonal incomplete Cholesky preconditioner
        tolerance       1e-08;  //Absolute tolerance
        relTol          0;  //Relative tolerance
    }
}

SIMPLE
{
}


