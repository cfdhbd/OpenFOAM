FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluidB";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 3;

boundaryField
{
    inletOther
    {
        type            zeroGradient;
    }
    outletOther
    {
        type            zeroGradient;
    }
    fluidB_to_fluidA
    {
        type            calculated;
        value           uniform 0;
    }
}


