FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluidA";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    inletOther
    {
        type            zeroGradient;
    }
    outletOther
    {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    fluidA_to_fluidB
    {
        type            calculated;
        value           uniform 0;
    }
}


